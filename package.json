{
  "name": "container-mm-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --fix .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest --config vitest.config.integration.ts",
    "test:integration:watch": "vitest --config vitest.config.integration.ts --watch",
    "test:integration:coverage": "vitest --config vitest.config.integration.ts --coverage",
    "validate": "pnpm run type-check && pnpm run check && pnpm run test",
    "analyze": "ANALYZE=true pnpm run build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:status": "tsx scripts/db-status.ts",
    "db:migrate:prod": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:force": "FORCE_SEED=true tsx scripts/seed.ts",
    "postbuild": "next-sitemap",
    "vercel-build": "pnpm run db:migrate:prod && pnpm run build",
    "deploy:check": "pnpm run validate && pnpm run db:status",
    "deploy:verify": "tsx scripts/verify-deployment.ts",
    "docker:up": "docker-compose --env-file .env.docker up -d",
    "docker:down": "docker-compose --env-file .env.docker down",
    "docker:logs": "docker-compose --env-file .env.docker logs -f",
    "docker:clean": "docker-compose --env-file .env.docker down -v --remove-orphans && docker system prune -f",
    "perf:audit": "lhci autorun",
    "perf:build": "pnpm build && pnpm start &",
    "perf:mobile": "lhci collect --preset=mobile --numberOfRuns=3",
    "perf:desktop": "lhci collect --preset=desktop --numberOfRuns=3"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "5.83.0",
    "@tanstack/react-query-devtools": "5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/axios": "^0.14.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.4",
    "exceljs": "^4.4.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.11",
    "nanoid": "^5.1.5",
    "next": "15.4.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-hook-form": "7.60.0",
    "server-only": "^0.0.1",
    "sonner": "2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "4.0.5",
    "zustand": "5.0.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.4",
    "@playwright/test": "^1.55.0",
    "@redux-devtools/extension": "^3.3.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testcontainers/postgresql": "^11.5.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.1.0",
    "next-sitemap": "^4.2.3",
    "playwright": "^1.55.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
